{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/webflow/[...path].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webflow/(.*)",
      "dest": "/api/webflow/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}

